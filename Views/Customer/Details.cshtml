@model WebApplication1.Models.Customer
@{
    ViewBag.Title = "Customer Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<li>Membership Type: @Model.MembershipType.Name</li>
@if(Model.BirthdayDate != null){
<li>Birthday Date: @Model.BirthdayDate.Value.ToString("dd-MM-yyyy")</li>
}

